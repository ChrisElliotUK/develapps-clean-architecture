{
	"name": "flutter-clean-architecture",
	"displayName": "Flutter clean architecture",
	"description": "This extension helps to develop flutter projects with clean architecture. Clone of https://github.com/fgadea/develapps-clean-architecture",
	"version": "1.0.1",
	"publisher": "Christopher Elliot",
	"repository": "https://github.com/ChrisElliotUK/flutter-clean-architecture.git",
	"icon": "https://github.com/ChrisElliotUK/develapps-clean-architecture/blob/3ce2d14371160e831d827482711fad0b5b39f1b4/develappscleanarchitecture/images/develapps_logo.png",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:develappscleanarchitecture.create_feature",
		"onCommand:develappscleanarchitecture.create_project_structure"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "develappscleanarchitecture.create_feature",
				"title": "Develapps CA: New Feature"
			},
			{
				"command": "develappscleanarchitecture.create_project_structure",
				"title": "Develapps CA: Create Project Structure"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "develappscleanarchitecture.create_feature",
					"group": "2_workspace"
				},
				{
					"when": "explorerResourceIsFolder",
					"command": "develappscleanarchitecture.create_project_structure",
					"group": "2_workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}